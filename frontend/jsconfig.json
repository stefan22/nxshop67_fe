{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "rootDir": "frontend",
    "skipLibCheck": true,
    "strict": false,
    "target": "es5"
  },
  "include": [
    "next-env.d.js",
    "**/*.js",
    "**/*.jsx"
  ],
  "exclude": ["node_modules"]
}
